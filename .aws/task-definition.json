{
    "family": "TaskDefinitionFamily",
    "networkMode": "awsvpc",
    "containerDefinitions": [
        {
            "name": "frontend-app",
            "image": "891376980037.dkr.ecr.us-east-1.amazonaws.com/altangerel.me:latest",
            "essential": true,
            "memory": 512,
            "cpu": 256,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80,
                    "protocol": "tcp"
                }
            ],
            "environment": [
                {
                    "name": "ENV_VAR_NAME",
                    "value": "ENV_VAR_VALUE"
                }
            ]
        }
    ],
    "requiresCompatibilities": ["FARGATE"],
    "cpu": "256",
    "memory": "512",
    "executionRoleArn": "arn:aws:iam::891376980037:role/YOUR_ECS_TASK_EXECUTION_ROLE",
    "taskRoleArn": "arn:aws:iam::891376980037:role/YOUR_ECS_TASK_ROLE"
}