{
  "compilerOptions": {
      "target": "es2018",
      "module": "esnext",
      "moduleResolution": "node",
      "lib": ["esnext", "esnext.asynciterable", "dom"],
      "esModuleInterop": true,
      "allowJs": true,
      "skipLibCheck": true,
      "strict": true,
      "noEmit": true,
      "baseUrl": ".",
      "paths": {
        "~/*": ["./*"],
        "@/*": ["./*"]
      },
      "types": ["@nuxt/types"]
    },
  "exclude": ["node_modules", ".nuxt", "dist"],
  "extends": "../.nuxt/tsconfig.server.json"
}
